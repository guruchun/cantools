<?xml version="1.0" encoding="utf-8"?>
<ToolConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <!-- HMI configuration -->
  <hmi>
    <layoutManager>
      <property key="Mode" value ="None"/>
      <property key="Width" value="1000"/>
      <property key="Height" value="800"/>
    </layoutManager>
    <helpPage>
      <!-- Source = {Online, Offline} -->
      <property key="Source" value ="Online"/>
      <!--
      Online & Internal: value ="http://10.120.14.48/redmine/projects/comm/wiki/"
      Online & External: value ="https://github.com/guruchun/fcp-public/blob/master/bop-tools/"
      Offline: value ="help"
      -->
      <property key="Location" value ="http://10.120.14.48/redmine/projects/comm/wiki/"/>
    </helpPage>
    <dataStore>
      <property key="InitialTags" value="config\bopepe-tag-defaults.txt"/>
      <property key="DescriptionTags" value="config\bopepe-tag-desc.txt"/>
    </dataStore>
    <!-- Log Windows-->
    <logViews>
      <property key="Device" value="bopepe-data.log"/>
      <property key="Console" value="bopepe-console.log"/>
    </logViews>
  </hmi>
</ToolConfig>